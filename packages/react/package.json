  "description": "put your ab testing on autopilot",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "prettier --write --parser typescript '**/*.{ts,tsx}' && eslint . --ext .ts,.tsx --ignore-path .gitignore --fix",
